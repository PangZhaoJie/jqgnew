<struts>
    <constant name="struts.multipart.saveDir" value="/tmp"></constant>
    <package name="overall" namespace="/overall" extends="default">
    <result-types>
			<result-type name="json" class="org.apache.struts2.json.JSONResult" />
		</result-types>
		<!-- 定义一个拦截器 -->
		<interceptors>
			<interceptor name="json"
				class="org.apache.struts2.json.JSONInterceptor" />
			<interceptor name="user" class="com.jqg.Interceptor.CkeckBackLogin">
			</interceptor>
			<interceptor name="authority"
				class="com.jqg.Interceptor.LoginInterceptor">
			</interceptor>
			<!-- 拦截器栈 -->
			<interceptor-stack name="teamwareStack">
				<interceptor-ref name="defaultStack" />
				<interceptor-ref name="json" />
				<interceptor-ref name="user" />
				<interceptor-ref name="authority" />
			</interceptor-stack>
		</interceptors>
    
         <!--查找所有广告 -->
         <action name="findAverts" class="com.jqg.struts.OverallAction" method="findAverts">
            <result>/back/1_avert_set.jsp</result>
             <interceptor-ref name="defaultStack" /> 
           <interceptor-ref name="authority" />  
               
         </action>
         <!-- 查找单个广告 -->
         <action name="findAvert" class="com.jqg.struts.OverallAction" method="findAvert">
