           <interceptor-ref name="authority" />  
         </action>
              <!-- 查找首页新标预告-->
         <action name="findlssh" class="com.jqg.struts.NewsAction" method="findlssh">
         
         <result>/back/9_uptherald.jsp</result>
           <interceptor-ref name="defaultStack" /> 
           <interceptor-ref name="authority" />  
         </action>
