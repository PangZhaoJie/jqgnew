       <action name="vidio" class="com.jqg.struts.UserMangerAction" method="vidio">
         <result>/back/3_list3.jsp</result>
          <interceptor-ref name="defaultStack" /> 
           <interceptor-ref name="authority" />  
         </action>
      <!--现场认证-->
        <action name="scene" class="com.jqg.struts.UserMangerAction" method="scene">
         <result>/back/3_list4.jsp</result>
          <interceptor-ref name="defaultStack" /> 
           <interceptor-ref name="authority" />  
         </action>
