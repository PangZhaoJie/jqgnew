        <action name="quota" class="com.jqg.struts.UserMangerAction" method="quota">
         <result>/back/3_list5.jsp</result>
          <interceptor-ref name="defaultStack" /> 
           <interceptor-ref name="authority" />  
         </action>
          <!--实名认证-->
        <action name="realname" class="com.jqg.struts.UserMangerAction" method="realname">
         <result>/back/3_namelist.jsp</result>
          <interceptor-ref name="defaultStack" /> 
