<struts>
  <constant name="struts.ognl.allowStaticMethodAccess" value="true" />
    
 
    <package name="borrow" namespace="/borrow" extends="default">
    
    
    	<!-- 定义一个拦截器 -->  
        <interceptors>  
            <interceptor name="boorow"  
                class="com.jqg.Interceptor.CkeckBackLogin">  
            </interceptor>  

            <!-- 拦截器栈 -->  
                <interceptor-stack name="teamwareStack">  
                <interceptor-ref name="defaultStack" />  
                 <interceptor-ref name="boorow" />  

            </interceptor-stack>  
        </interceptors>  
       <!--  <default-interceptor-ref name="teamwareStack"/>  同一个package默认此拦截器-->
        
         <!-- 定义全局Result -->  
        <global-results>  
            <!-- 当返回login视图名时，转入/login.jsp页面 -->  
            <result name="login"  type="redirect">/login/login.jsp</result>  
        </global-results> 
 		<!-- 发标页面初始化 -->
        <action name="toLssuing" class="com.jqg.struts.LssuingAction" method="toLssuing">
        	<result>/borrow/borrow.jsp</result>	  
            <interceptor-ref name="teamwareStack" /><!-- 使用此拦截器 -->
        </action>
        <!-- 保存发标 -->
       <action name="saveLssuing" class="com.jqg.struts.LssuingAction" method="saveLssuing">
       <interceptor-ref name="teamwareStack" /><!-- 使用此拦截器 -->
	        <result name="success" >/jump.jsp</result>
         </action>
 		<!-- 自动投标页面初始化 -->
