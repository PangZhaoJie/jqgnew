<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jqg.pojo.AppPhotos" table="appphotos" >
        <id name="appId" type="java.lang.Integer">
            <column name="appId" />
            <generator class="native" />
        </id>
        <property name="userName" type="java.lang.String">
            <column name="userName" length="20" />
        </property>
        <property name="title" type="java.lang.String">
            <column name="title" length="20" />
        </property>
        <property name="pathImages" type="java.lang.String">
            <column name="pathImages" length="100" />
        </property>
        <property name="type" type="java.lang.Integer">
            <column name="type" length="5" />
        </property>
        <property name="url" type="java.lang.String">
            <column name="url" length="100"/>
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" length="1000" />
        </property>
        <property name="publishTime" type="java.sql.Timestamp">
            <column name="publishTime" length="19" />
        </property>
    </class>
</hibernate-mapping>
