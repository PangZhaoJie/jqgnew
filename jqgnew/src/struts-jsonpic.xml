<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <constant name="struts.multipart.saveDir" value="/tmp"></constant>
    <package name="pic" namespace="/pic" extends="json-default">
        	 
        <!-- 图片ajax上传，无刷新 -->
        <action name="toSavePic" class="com.jqg.struts.RechargeAction" method="toSavePic">
			<result type="json" name="success">
        		<param name="contentType">
        			text/html
        		</param>
        	</result>
        	<result type="json" name="error">
        		<param name="contentType">
        			text/html
        		</param>
        	</result>
 		</action>
		  <action name="toSaveP" class="com.jqg.struts.BackLssuingAction" method="toSavePhoto">
			<result type="json" name="success">
        		<param name="contentType">
        			text/html
        		</param>
        	</result>
        	<result type="json" name="error">
        		<param name="contentType">
        			text/html
        		</param>
        	</result>
 		</action>
 		
		  <action name="toSave" class="com.jqg.struts.BackLssuingAction" method="SavePhoto">
			<result type="json" name="success">
        		<param name="contentType">
        			text/html
        		</param>
        	</result>
        	<result type="json" name="error">
        		<param name="contentType">
        			text/html
        		</param>
        	</result>
 		</action>
		  
		<action name="toDeleteP" class="com.jqg.struts.BackLssuingAction" method="toDeleteP">
			<result type="json" name="success">
        		<param name="contentType">
        			text/html
        		</param>
        	</result>
        	<result type="json" name="error">
        		<param name="contentType">
        			text/html
        		</param>
        	</result>
 		</action> 
		 
    </package>
</struts>
