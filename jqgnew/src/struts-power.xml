<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="power" namespace="/power" extends="default">
        <!-- 用户组权限管理-->
        <action name="searchRoles" class="com.jqg.struts.PowerAction" method="searchRoles">
           <result>/back/81_list.jsp</result>
           <interceptor-ref name="defaultStack" /> 
           <interceptor-ref name="authority" />  
        </action>
        <action name="savePowers" class="com.jqg.struts.PowerAction" method="savePowers">
           <result>/back/81_list.jsp</result>
           <interceptor-ref name="defaultStack" /> 
           <interceptor-ref name="authority" />  
        </action>
        <action name="editPowers" class="com.jqg.struts.PowerAction" method="editPowers">
           <result>/back/8_edit.jsp</result>
           <interceptor-ref name="defaultStack" /> 
           <interceptor-ref name="authority" />   
        </action>
        <action name="delRole" class="com.jqg.struts.PowerAction" method="delRole">
           <result>/back/81_list.jsp</result>
           <interceptor-ref name="defaultStack" /> 
           <interceptor-ref name="authority" />  
        </action>
    </package>
</struts>
